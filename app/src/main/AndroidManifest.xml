<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mybar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".Inicio"
                 android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".actividadesSecundaris.Recargas"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".actividadesSecundaris.Pagos"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".actividadesSecundaris.Creditos"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".actividadesSecundaris.Transferir"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".actividadesSecundaris.VerMovimientos"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".actividadesSecundaris.TarjetasDeCredito"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>